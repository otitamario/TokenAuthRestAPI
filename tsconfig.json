{
  "compilerOptions": {                        
    "target": "es2019",                               
    "module": "commonjs",                           
    "outDir": "./dist",                             
    "rootDir": "./src",
    "typeRoots": ["./src/@types",
    "./node_modules/@types"],                             
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,                       
  },
  "exclude":[
    "./node_modules"
  ]
}